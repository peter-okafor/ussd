{
    "name": "okaforpeter/ussd",
    "description": "USSD Adaptor for Laravel PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Okafor",
            "email": "peter.obinna.okafor@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": ">=5.8",
        "ext-json": "*",
        "ext-simplexml": "*",
        "tnmdev/ussd-simulator": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TNM\\USSD\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TNM\\USSD\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TNM\\USSD\\UssdServiceProvider"
            ]
        }
    }
}
